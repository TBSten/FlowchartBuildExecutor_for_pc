<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fbe.exe.SettingController">
   <children>
      <BorderPane prefHeight="497.0" prefWidth="827.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <bottom>
            <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <buttons>
                <Button fx:id="bt_ok" mnemonicParsing="false" text="OK" />
                  <Button fx:id="bt_can" mnemonicParsing="false" text="キャンセル" />
              </buttons>
            </ButtonBar>
         </bottom>
         <center>
            <VBox BorderPane.alignment="CENTER">
               <children>
                  <Label text="表示タイプ" />
                  <ScrollPane>
                     <content>
                        <HBox fx:id="hb_viewType" prefHeight="150.0" prefWidth="200.0">
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding></HBox>
                     </content>
                  </ScrollPane>
                  <Label fx:id="lb_desc" text="[description]">
                     <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </padding></Label>
                  <Pane prefHeight="44.0" prefWidth="827.0" />
                  <Label text="オプション" />
                  <ScrollPane fx:id="sp_option" hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
