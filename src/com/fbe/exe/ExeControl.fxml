<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="com.fbe.exe.ControlController">
   <children>
      <VBox>
         <children>
            <Button fx:id="bt_exe" mnemonicParsing="false" text="実行" />
            <Button fx:id="bt_stop" mnemonicParsing="false" text="一時停止" />
            <Button fx:id="bt_finish" mnemonicParsing="false" text="終了" />
         </children>
      </VBox>
   </children>
</AnchorPane>
